{
  "_template": "/video/VideoPlaylistEnhancement.hbs",
  "title": "Video Playlist Module",
  "player": {
    "_include": "/video/HTML5VideoPlayer.json"
  },
  "playlist": [
    {
      "_include": "/video/VideoPlaylistItem.json"
    },
    {
      "_include": "/video/VideoPlaylistItem.json",
      "player": {
        "_include": "/video/HTML5VideoPlayer.json",
        "sources": [
          {
              "_template": "HTML5VideoPlayerSource.hbs",
              "src": "ttp://media.w3.org/2010/05/sintel/trailer.mp4",
              "type": "video/mp4"
          },
          {
              "_template": "HTML5VideoPlayerSource.hbs",
              "src": "http://media.w3.org/2010/05/sintel/trailer.ogv",
              "type": "video/ogg"
          }
      ]
      },
      "duration": "5 min",
      "title": "{{words(10)}}",
      "description": "{{words(20)}}"
    },
    {
      "_include": "/video/VideoPlaylistItem.json",
      "player": {
        "_include": "/youtube/YouTubeVideoPlayer.json"
      }
    },
    {
      "_include": "/video/VideoPlaylistItem.json"
    },
    {
      "_include": "/video/VideoPlaylistItem.json",
      "player": {
        "_include": "/youtube/YouTubeVideoPlayer.json"
      }
    },
    {
      "_include": "/video/VideoPlaylistItem.json",
      "player": {
        "_include": "/vimeo/VimeoVideoPlayer.json"
      }
    },
    {
      "_include": "/video/VideoPlaylistItem.json"
    },
    {
      "_include": "/video/VideoPlaylistItem.json",
      "player": {
        "_include": "/vimeo/VimeoVideoPlayer.json"
      }
    }
  ]
}
